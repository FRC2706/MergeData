{
  "Pre-Match": [
    {
      "name": "Scouter Initials",
      "required": true,
      "type": "text"
    },
    {
      "name": "Event",
      "defaultValue": "insert code for north bay",
      "required": true,
      "type": "text"
    },
    {
      "name": "Match #",
      "required": true,
      "type": "number"
    },
    {
      "name": "Robot",
      "choices": ["Red 1", "Red 2", "Red 3", "Blue 1", "Blue 2", "Blue 3"],
      "required": true,
      "type": "radio"
    },
    {
      "name": "Team #",
      "required": false,
      "type": "number"
    }
  ],
  "Autonomous": [
    {
      "name": "Leave Starting Zone",
      "type": "bool"
    },
    {
      "name": "Notes Picked Up",
      "type": "counter"
    },
    {
      "name": "Notes Dropped",
      "type": "counter"
    },
    {
      "name": "Auto Amp Scores",
      "type": "counter"
    },
    {
      "name": "Auto Speaker Scores",
      "type": "counter"
    }
  ],
  "TeleOp": [
    {
      "name": "Picked UP: Ground",
      "type": "counter"
    },
    {
      "name": "Picked Up: Source",
      "type": "counter"
    },
    {
      "name": "Notes: Dropped",
      "type": "counter"
    },
    {
      "name": "Amp: Scores",
      "type": "counter"
    },
    {
      "name": "Amp Misses",
      "type": "counter"
    },
    {
      "name": "Speaker: Scores",
      "type": "counter"
    },
    {
      "name": "Speaker: Misses",
      "type": "counter"
    },
    {
      "name": "Pickup From",
      "type": "radio",
      "choices": ["Source", "Floor", "Both", "Not Attempted"],
      "required": true
    },
    {
      "name": "Died :( [time is logged when clicked]",
      "type": "bool"
    }
  ],
  "EndGame": [
    {
      "name": "Final Status",
      "type": "radio",
      "required": true,
      "choices": ["Parked (touching stage)", "Onstage (climb)", "Harmony (climb with friend)", "Onstage (Spotlit)", "Attempted but failed :(", "Not attempted"]
    },
    {
      "name": "Note in Trap",
      "type": "radio",
      "required": true,
      "choices": ["Not attempted", "Yes", "Attempted, but failed"]
    }
  ],
  "PostMatch": [
    {
      "name": "Driver Skill",
      "type": "radio",
      "required": true,
      "choices": ["Not Observed", "Not Effective", "Average", "Very Effective"]
    },
    {
      "name": "Defense Rating",
      "type": "radio",
      "required": true,
      "choices": ["Below Average", "Average", "Good", "Excellent", "Did not play defense :("]
    
    },
    {
      "name": "Speed Rating",
      "type": "radio",
      "required": true,
      "choices": ["1 (slow)", "2", "3", "4", "5 (fast)"]
    },
    {
      "name": "Tippy, looks like it'll fall over",
      "type": "bool"
    },
    {
      "name": "Dropped Notes (>2)",
      "type": "bool"
    },
    {
      "name": "Make good alliance partner?",
      "type": "bool"
    },
    {
      "name": "Comments",
      "type": "text"
    }
  ]
}